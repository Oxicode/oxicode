{
  "extends":[
    "eslint:recommended",
    "standard",
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "camelcase": "warn",
    "space-before-function-paren": "off",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "indent": [
      "error",
      2
    ]
  },
  "plugins": [
    "simple-import-sort"
  ]
}
