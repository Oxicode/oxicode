{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
      "eslint:recommended",
      "standard",
      "next/core-web-vitals"
  ],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  },
  "plugins": ["simple-import-sort"]
}
